{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tizenbrew-device-manager",
  "version": "1.0.1",
  "identifier": "io.gh.reisxd.tizenbrew-device-manager.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "TizenBrew Device Manager",
        "minWidth": 1025,
        "minHeight": 768
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}